<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="house" class="ru.philimonov.springcourse.beans.House" autowire="byType">
        <property name="countOfFlats" value="200"/>
        <property name="countOfFloors" value="16"/>
    </bean>

    <bean id="address" class="ru.philimonov.springcourse.beans.Address" autowire-candidate="false">
        <constructor-arg index="0" value="Migaja"/>
        <constructor-arg index="1" value="321"/>
    </bean>

    <bean id="address2" class="ru.philimonov.springcourse.beans.Address">
        <constructor-arg index="0" value="Nelson"/>
        <constructor-arg index="1" value="6658"/>
    </bean>
</beans>